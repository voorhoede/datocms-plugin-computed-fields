{
  "name": "datocms-plugin-computed-fields",
  "homepage": "https://github.com/voorhoede/datocms-plugin-computed-fields",
  "version": "2.4.6",
  "description": "This plugin allows to compute and define a field value based on other fields",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "cross-env BROWSER='none' PUBLIC_URL='/' react-scripts start",
    "build": "cross-env PUBLIC_URL='.' react-scripts build",
    "eject": "react-scripts eject",
    "prepack": "npm run build",
    "postversion": "git push --follow-tags",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.1.4",
    "@codemirror/theme-one-dark": "^6.1.1",
    "@datocms/cma-client-browser": "^1.2.12",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/lodash": "^4.14.192",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@uiw/react-codemirror": "^4.19.11",
    "datocms-plugin-sdk": "^0.7.4",
    "datocms-react-ui": "^0.7.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "cross-env": "^7.0.3",
    "jest": "^29.6.1",
    "react-scripts": "^5.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "files": [
    "build",
    "docs"
  ],
  "keywords": [
    "datocms",
    "datocms-plugin",
    "field-editor",
    "json-field",
    "text-field",
    "boolean-field",
    "float-field",
    "integer-field",
    "string-field",
    "links-field",
    "link-field",
    "date-field",
    "date-time-field",
    "video-field",
    "color-field",
    "seo-field",
    "lat-lon-field",
    "structured-text",
    "computed",
    "computed fields",
    "function",
    "functions",
    "fields",
    "text",
    "boolean"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/voorhoede/datocms-plugin-computed-fields.git"
  },
  "author": "Voorhoede <victor@voorhoede.nl>",
  "license": "ISC",
  "datoCmsPlugin": {
    "title": "Computed Fields",
    "previewImage": "docs/preview.gif",
    "coverImage": "docs/cover.png",
    "entryPoint": "build/index.html",
    "permissions": [
      "currentUserAccessToken"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
